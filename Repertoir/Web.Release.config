<?xml version="1.0" encoding="utf-8"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <connectionStrings xdt:Transform="Replace">
    <add name="RepertoirContext" connectionString="dummy" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add xdt:Locator="Condition([@key='Environment'])" key="Environment" value="Release" xdt:Transform="Replace" />
  </appSettings>
  <system.web>
    <compilation xdt:Transform="RemoveAttributes(debug)" />
    <customErrors defaultRedirect="~/content/error.html" mode="Off" xdt:Transform="Replace" />
  </system.web>
</configuration>
